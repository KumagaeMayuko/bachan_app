<div id="diaries-show">
  <p id="notice"><%= notice %></p>

  <div class='blog'>
    <h1>BLOG</h1>
  </div>

  <div class='date'>
    <p><%= @diary.created_at.strftime("%Y.%m.%d") %></p>
  </div>

  <div class='title'>
    <p><%= @diary.title %></p>
  </div>

  <div class='content'>
    <p><%= @diary.content %></p>
  </div>

  <div class='picture'>
    <p>
    <%= image_tag @diary.picture,class:'img'%>
    </p>
  </div>

  <div class='back'>
    <p>
    < <%= link_to 'home', root_path %> |
    </p>
  </div>
</
